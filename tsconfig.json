{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "resolvePackageJsonExports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "target": "ES2023",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/swagger-aggregator": [
        "apps/swagger-aggregator/src"
      ],
      "@app/swagger-aggregator/*": [
        "apps/swagger-aggregator/src/*"
      ],
      "@app/auth-service": [
        "apps/auth-service/src"
      ],
      "@app/auth-service/*": [
        "apps/auth-service/src/*"
      ],
      "@app/player-service": [
        "apps/player-service/src"
      ],
      "@app/player-service/*": [
        "apps/player-service/src/*"
      ],
      "@app/building-service": [
        "apps/building-service/src"
      ],
      "@app/building-service/*": [
        "apps/building-service/src/*"
      ],
      "@app/combat-progress-service": [
        "apps/combat-progress-service/src"
      ],
      "@app/combat-progress-service/*": [
        "apps/combat-progress-service/src/*"
      ],
      "@app/scheduler-service": [
        "apps/scheduler-service/src"
      ],
      "@app/scheduler-service/*": [
        "apps/scheduler-service/src/*"
      ],
      "@app/game-server": [
        "apps/game-server/src"
      ],
      "@app/game-server/*": [
        "apps/game-server/src/*"
      ],
      "@app/analytics-service": [
        "apps/analytics-service/src"
      ],
      "@app/analytics-service/*": [
        "apps/analytics-service/src/*"
      ],
      "@lib/shared": [
        "libs/shared/src"
      ],
      "@lib/shared/*": [
        "libs/shared/src/*"
      ],
      "@lib/lib-player": [
        "libs/lib-player/src"
      ],
      "@lib/lib-player/*": [
        "libs/lib-player/src/*"
      ],
      "@lib/lib-building": [
        "libs/lib-building/src"
      ],
      "@lib/lib-building/*": [
        "libs/lib-building/src/*"
      ],
      "@lib/lib-game-server": [
        "libs/lib-game-server/src"
      ],
      "@lib/lib-game-server/*": [
        "libs/lib-game-server/src/*"
      ],
      "@lib/lib-combat-progress": [
        "libs/lib-combat-progress/src"
      ],
      "@lib/lib-combat-progress/*": [
        "libs/lib-combat-progress/src/*"
      ],
      "@lib/lib-analytics": [
        "libs/lib-analytics/src"
      ],
      "@lib/lib-analytics/*": [
        "libs/lib-analytics/src/*"
      ],
      "@prisma/meta": [
        "prisma/users/generated"
      ],
      "@prisma/catalog": [
        "prisma/balance/generated"
      ]
    }
  }
}
