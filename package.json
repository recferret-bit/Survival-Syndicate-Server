{
  "name": "survival-syndicate-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "nest build --all",
    "build:all": "npm run build swagger-aggregator && npm run build auth-service && npm run build player-service && npm run build building-service && npm run build combat-progress-service && npm run build scheduler-service && npm run build game-server && npm run build analytics-service",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "lint:fix": "eslint \"apps/**/*.ts\" \"libs/**/*.ts\" --fix",
    "prepare": "husky install",

    "start:swagger-aggregator": "nest start swagger-aggregator",
    "start:swagger-aggregator:dev": "nest start swagger-aggregator --watch",
    "start:swagger-aggregator:debug": "nest start swagger-aggregator --debug --watch",
    "start:auth-service": "nest start auth-service",
    "start:auth-service:dev": "nest start auth-service --watch",
    "start:auth-service:debug": "nest start auth-service --debug --watch",
    "start:player-service": "nest start player-service",
    "start:player-service:dev": "nest start player-service --watch",
    "start:player-service:debug": "nest start player-service --debug --watch",
    "start:building-service": "nest start building-service",
    "start:building-service:dev": "nest start building-service --watch",
    "start:building-service:debug": "nest start building-service --debug --watch",
    "start:combat-progress-service": "nest start combat-progress-service",
    "start:combat-progress-service:dev": "nest start combat-progress-service --watch",
    "start:combat-progress-service:debug": "nest start combat-progress-service --debug --watch",
    "start:scheduler-service": "nest start scheduler-service",
    "start:scheduler-service:dev": "nest start scheduler-service --watch",
    "start:scheduler-service:debug": "nest start scheduler-service --debug --watch",
    "start:game-server": "nest start game-server",
    "start:game-server:dev": "nest start game-server --watch",
    "start:game-server:debug": "nest start game-server --debug --watch",
    "start:analytics-service": "nest start analytics-service",
    "start:analytics-service:dev": "nest start analytics-service --watch",
    "start:analytics-service:debug": "nest start analytics-service --debug --watch",
    "start:local": "concurrently -n gateway,auth,player,building,combat,scheduler,game,analytics \"npx dotenv-cli -e .env.local -- nest start swagger-aggregator\" \"npx dotenv-cli -e .env.local -- nest start auth-service\" \"npx dotenv-cli -e .env.local -- nest start player-service\" \"npx dotenv-cli -e .env.local -- nest start building-service\" \"npx dotenv-cli -e .env.local -- nest start combat-progress-service\" \"npx dotenv-cli -e .env.local -- nest start scheduler-service\" \"npx dotenv-cli -e .env.local -- nest start game-server\" \"npx dotenv-cli -e .env.local -- nest start analytics-service\"",
    "start:prod": "node dist/apps/swagger-aggregator/main",

    "prisma:generate": "npm run prisma:generate:meta && npm run prisma:generate:catalog",
    "prisma:generate:meta": "prisma generate --schema=prisma/meta/schema.prisma",
    "prisma:generate:catalog": "prisma generate --schema=prisma/catalog/schema.prisma",
    "prisma:migrate:dev": "npm run prisma:migrate:dev:meta && npm run prisma:migrate:dev:catalog",
    "prisma:migrate:dev:meta": "prisma migrate dev --schema=prisma/meta/schema.prisma",
    "prisma:migrate:dev:catalog": "prisma migrate dev --schema=prisma/catalog/schema.prisma",
    "prisma:migrate:deploy": "npm run prisma:migrate:deploy:meta && npm run prisma:migrate:deploy:catalog",
    "prisma:migrate:deploy:meta": "prisma migrate deploy --schema=prisma/meta/schema.prisma",
    "prisma:migrate:deploy:catalog": "prisma migrate deploy --schema=prisma/catalog/schema.prisma",
    "prisma:studio:meta": "prisma studio --schema=prisma/meta/schema.prisma",
    "prisma:studio:catalog": "prisma studio --schema=prisma/catalog/schema.prisma",
    "prisma:seed": "ts-node prisma/seed.ts",
    "prisma:reset": "npm run prisma:reset:meta && npm run prisma:reset:catalog && npm run prisma:seed",
    "prisma:reset:meta": "prisma migrate reset --schema=prisma/meta/schema.prisma --force",
    "prisma:reset:catalog": "prisma migrate reset --schema=prisma/catalog/schema.prisma --force",

    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/swagger-aggregator/test/jest-e2e.json",

    "docker:infra": "docker compose -f docker/docker-compose.infra.yml up -d",
    "docker:infra:down": "docker compose -f docker/docker-compose.infra.yml down",
    "docker:full": "docker compose -f docker/docker-compose.full.yml up -d",
    "docker:full:down": "docker compose -f docker/docker-compose.full.yml down"
  },
  "dependencies": {
    "@anatine/zod-nestjs": "2.0.12",
    "@nestjs/bullmq": "11.0.4",
    "@nestjs/common": "11.0.1",
    "@nestjs/config": "4.0.0",
    "@nestjs/core": "11.0.1",
    "@nestjs/cqrs": "11.0.1",
    "@nestjs/jwt": "11.0.1",
    "@nestjs/microservices": "11.0.1",
    "@nestjs/platform-express": "11.0.1",
    "@nestjs/schedule": "6.0.1",
    "@nestjs/swagger": "11.0.3",
    "@nestjs/terminus": "11.0.0",
    "@prisma/adapter-pg": "7.0.0",
    "@prisma/client": "7.0.0",
    "@willsoto/nestjs-prometheus": "6.0.2",
    "axios": "1.9.0",
    "bignumber.js": "9.3.0",
    "bullmq": "5.63.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "hbs": "4.2.0",
    "http-proxy-middleware": "3.0.5",
    "ioredis": "5.8.2",
    "marked": "17.0.1",
    "nats": "2.20.0",
    "pg": "8.16.3",
    "prom-client": "15.1.3",
    "puppeteer": "24.35.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "uuid": "9.0.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "11.0.12",
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "11.0.1",
    "@types/express": "5.0.0",
    "@types/http-proxy-middleware": "1.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.7",
    "@types/supertest": "6.0.2",
    "@types/uuid": "9.0.8",
    "concurrently": "^9.2.1",
    "cross-env": "10.1.0",
    "dotenv-cli": "11.0.0",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.2",
    "globals": "16.0.0",
    "husky": "^9.1.7",
    "jest": "29.7.0",
    "pdf-parse": "^2.4.5",
    "prettier": "3.4.2",
    "prisma": "7.0.0",
    "rimraf": "6.0.1",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.7.3",
    "typescript-eslint": "8.20.0"
  }
}
